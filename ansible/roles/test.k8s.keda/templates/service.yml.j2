apiVersion: v1
kind: Service
metadata:
  name: http-request-total
  namespace: "{{ K8S_DEFAULE_NAMESPACE }}"
  labels:
    app: http-request-total
  annotations:
    prometheus.io/port: "{{ metrics_port }}"
    prometheus.io/scrape: "true"
spec:
  selector:
    app: http-request-total
  ports:
  - port: {{ metrics_port }}
    protocol: TCP
    targetPort: metrics
